{
  "name": "bug-tracker",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
  "install-all": "cd client && npm install && cd server && npm install",
  "start": "concurrently \"npm start --prefix server\" \"npm start --prefix client\""
},
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
